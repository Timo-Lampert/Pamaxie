      "commandName": "WSL2",
      "launchBrowser": true,
      "launchUrl": "http://localhost",
      "environmentVariables": {
        "ASPNETCORE_URLS": "http://0.0.0.0",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "distributionName": ""
